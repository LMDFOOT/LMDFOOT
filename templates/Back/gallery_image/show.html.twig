{% extends 'Back/base.html.twig' %}
{% block title %}{{ 'gallery.show'|trans }}{% endblock %}

{% block body %}
    <table class="table">
        <tbody>
            <tr>
                <th>ImageName</th>
                <td>{{ gallery_image.imageName }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ gallery_image.createdAt }}</td>
            </tr>
            <tr>
                <th>Published</th>
                <td>{{ gallery_image.published ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('gallery_image_index') }}">back to list</a>

    <a href="{{ path('gallery_image_edit', {'id': gallery_image.id}) }}">edit</a>

    {{ include('back/gallery_image/_delete_form.html.twig') }}
{% endblock %}
